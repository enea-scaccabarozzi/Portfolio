{
	"name": "portfolio",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/portfolio",
	"projectType": "application",
	"tags": [],
	"// targets": "run `nx show project portfolio --web` to see all the targets for this project",
	"targets": {
		"dev": {
			"executor": "nx:run-commands",
			"options": {
				"command": "sst bind next dev",
				"cwd": "apps/portfolio"
			}
		},
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"command": "open-next build --build-command \"next build\"",
				"cwd": "apps/portfolio"
			}
		},
		"build:analyze": {
			"executor": "nx:run-commands",
			"options": {
				"command": "open-next build --build-command \"WEB_BUNDLE_ANALYZE=true next build\"",
				"cwd": "apps/portfolio"
			}
		}
	}
}
